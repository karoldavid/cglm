{
  "version": 1,
  "projects": {
    "auth": {
      "root": "apps/auth",
      "sourceRoot": "apps/auth/src",
      "projectType": "application",
      "prefix": "auth",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/auth",
            "main": "apps/auth/src/main.ts",
            "tsConfig": "apps/auth/tsconfig.app.json",
            "assets": ["apps/auth/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/auth/src/environments/environment.ts",
                  "with": "apps/auth/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "auth:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/auth/tsconfig.app.json",
              "apps/auth/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/auth/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/auth/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "attendance": {
      "root": "apps/attendance",
      "sourceRoot": "apps/attendance/src",
      "projectType": "application",
      "prefix": "attendance",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/attendance",
            "main": "apps/attendance/src/main.ts",
            "tsConfig": "apps/attendance/tsconfig.app.json",
            "assets": ["apps/attendance/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/attendance/src/environments/environment.ts",
                  "with": "apps/attendance/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "attendance:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/attendance/tsconfig.app.json",
              "apps/attendance/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/attendance/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/attendance/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "attendee": {
      "root": "apps/attendee",
      "sourceRoot": "apps/attendee/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/attendee",
            "index": "apps/attendee/src/index.html",
            "main": "apps/attendee/src/main.tsx",
            "polyfills": "apps/attendee/src/polyfills.ts",
            "tsConfig": "apps/attendee/tsconfig.app.json",
            "assets": [
              "apps/attendee/src/favicon.ico",
              "apps/attendee/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/attendee/src/environments/environment.ts",
                  "with": "apps/attendee/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "attendee:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "attendee:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/attendee/tsconfig.app.json",
              "apps/attendee/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/attendee/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/attendee/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "attendee-e2e": {
      "root": "apps/attendee-e2e",
      "sourceRoot": "apps/attendee-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/attendee-e2e/cypress.json",
            "tsConfig": "apps/attendee-e2e/tsconfig.e2e.json",
            "devServerTarget": "attendee:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "attendee:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/attendee-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/attendee-e2e/**/*"]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json",
              "libs/ui/.storybook/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui",
            "config": {
              "configFolder": "libs/ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-e2e": {
      "root": "apps/ui-e2e",
      "sourceRoot": "apps/ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-e2e/cypress.json",
            "tsConfig": "apps/ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ui:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/ui-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/ui-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "auth",
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  }
}
