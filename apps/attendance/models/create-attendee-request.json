{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "CreateAttendeeRequest",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$",
      "minLength": 5,
      "maxLength": 20
    },
    "email": {
      "type": "string",
      "format": "email"
    }
  },
  "required": ["name", "email"],
  "additionalProperties": false
}
